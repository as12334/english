{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "types": ["@cloudflare/workers-types"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "skipLibCheck": true,
    "composite": true,
    "outDir": "./.tsbuild"
  },
  "include": ["./**/*.ts"]
}
